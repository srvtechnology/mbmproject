								<button type="submit" class="btn btn-success w-25">SEARCH</button>

						</div>
					</div>
				</div>
			</form> -->
			<div class="d-flex justify-content-between mb-2">
				<h4 class="h4">SPAREPART <span>LIST</span></h4>
				<a href="/sparepart_cr" class="btn btn-danger btn-create btn-sm">+ Create Sparepart</a>
			</div>
			<div class="card-body">
			<div class="table-responsive">
				<table id="basic-datatables" class="display table table-striped table-hover">
				<thead>
					<tr>
						<th>Id</th>
						<th>Name</th>
						<th>Price</th>
						<th>Product Name</th>
						<th class="action">Action</th>
					</tr>
				</thead>
				<tbody>
					<% pspareList.forEach(function(pspare){ %>
					<tr>
						<td><%= pspare.PSPARE_ID %></td>
						<td><%= pspare.SPNAME %></td>
						<td><%= pspare.PRICE %></td>
						<td><a href="/productdtl/<%= pspare.PRODUCT_ID %>"><%= pspare.PRDNAME %></a></td>
						<td>
							<a href="/sparepartbyId/Id/<%= pspare.PSPARE_ID %>" class="btn btn-dark btn-sm">View</a>
						</td>
					</tr>
					<% }); %>
				</tbody>
			</table>
			</div>
			</div>
			<!-- <div class="pagination">
  <a href="/sparepart/pageNo/1">&laquo;</a>
  <a href="/sparepart/pageNo/1" class="active">1</a>
